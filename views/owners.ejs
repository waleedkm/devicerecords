<%include partials/header%>

<div class="jumbotron">
<table class = "table" style="width:90%">
    <tr>
    <th scope="col">Name</th>
    <th scope="col">Department</th> 
  </tr>
<%owner.forEach(function(dev){ %>
 
 <tr>
    <td><%=dev.name%></td>
    <td><%=dev.department%></td> 
    <form method="DELETE">
    <td><button class="btn btn-primary btn-sm" method="DELETE" type="submit" formaction="/owners/delete/<%=dev._id%>" >Delete</button></td></form>
  </tr>


<%})%>
</table>
</div>
<div class="jumbotron">
    <form method="GET">
    <button class="btn btn-primary btn-lg" method="GET" type="submit" formaction="/owners/new" >Add a User</button>
    </form>
</div>    

<%include partials/footer%>