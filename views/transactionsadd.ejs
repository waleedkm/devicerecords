<%include partials/header%>
<div class="jumbotron">
    
    <form action="/transactionadd" method="POST">
        <div class="form-group">
        <input class="form-control" type="text" name="transactions[name]" placeholder="Name">
        </div>
        <div class="form-group">
        <input class="form-control" type="number" name="transactions[trasactionid]" placeholder="ID" required>
        </div>
        <div class="form-group">
        <input class="form-control" type="date" name="transactions[date]" placeholder="Date" required>
        </div>
        <div class="form-group">
        
                    <select class="form-group custom-select" id="inputGroupSelect02" name="transactions[type]" required>
                
                    <option value="towh">To WH</option>
                    <option value="torma">To RMA</option>
                    
               
              
        
            </select>
        
        </div>
    <table class="table" style="width:90%">
        <tr>
            <th scope="col">Select</th>
            <th scope="col">Name</th>
            <th scope="col">1c</th> 
  
            
        </tr>
      <%devices.forEach(function(dev){%>
        <tr>
            <td><input type="checkbox" name="transactions[devices][]" value=<%=dev.id%>></th>
                <td><%=dev.name%></td>
                <td><%=dev.onecid%></td> 

 
        </tr>

      <%})%>
    </table>
    
    <button class="form-control btn btn-primary btn-lg" type="submit">Submit</button>
    </form>
    <input type="button" onClick="window.print()" value="Print This Page"/>
   
</div>












<%include partials/footer%>